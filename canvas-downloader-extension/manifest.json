{
    "manifest_version": 3,
    "name": "Canvas One-Click Downloader",
    "version": "0.3.0",
    "description": "Download Canvas course materials into organized folders",
    "permissions": [
        "storage",
        "downloads",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "service_worker.js"
    }
}